# Platform
CONFIG_MACH_XIAOMI_SM8150=y

# Android Low Memory Killer
# CONFIG_ANDROID_LOW_MEMORY_KILLER is not set

# DT Overlay
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# QCACLD
CONFIG_QCA_CLD_WLAN=y

# pstore
CONFIG_CMDLINE="ramoops_memreserve=4M"
CONFIG_CMDLINE_EXTEND=y
CONFIG_PSTORE=y
CONFIG_PSTORE_CONSOLE=y
CONFIG_PSTORE_PMSG=y
CONFIG_PSTORE_RAM=y

# Touchscreen
# CONFIG_TOUCHSCREEN_ST is not set

# IR
CONFIG_IR_SPI_MI=y
CONFIG_LIRC=y
CONFIG_RC_CORE=y
CONFIG_RC_DECODERS=y
CONFIG_MEDIA_RC_SUPPORT=y
CONFIG_RC_DEVICES=y

# MGLRU
CONFIG_LRU_GEN=y
CONFIG_LRU_GEN_ENABLED=y

# Pixel Thermal
CONFIG_USB_OVERHEAT_MITIGATION=y
CONFIG_THERMAL_EMULATION=y

# Power
# CONFIG_SMB1355_SLAVE_CHARGER is not set
# CONFIG_SMB1390_CHARGE_PUMP_PSY is not set
CONFIG_SMB1390_CHARGE_PUMP=y

# spk-id
CONFIG_HAS_IOMEM=y
CONFIG_MFD_SPK_ID=y

# TCP/Net
CONFIG_TCP_CONG_ADVANCED=y
CONFIG_TCP_CONG_BBR=y
CONFIG_DEFAULT_BBR=y
CONFIG_NET_SCH_FQ_CODEL=y
CONFIG_NET_SCH_DEFAULT=y
CONFIG_DEFAULT_FQ_CODEL=y

# TEE
CONFIG_TRUSTY=y

# ZRAM Writeback
CONFIG_ZRAM_WRITEBACK=y
